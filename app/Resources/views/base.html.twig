<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('bundles/plateformeanimationterritoire/css/style.css') }}"/>
        <link rel="stylesheet" href="{{ asset('bundles/plateformeanimationterritoire/css/bootstrap.min.css') }}"/>
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('drug.jpg') }}" />
</head>
<body>

{% block navbar %}
    {{  include("::navbar.html.twig") }}
{% endblock %}
{% block body %}{%  endblock %}
{% block javascripts %}
    <script type="text/javascript" src="{{ asset('bundles/plateformeanimationterritoire/js/jquery-1.11.2.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/plateformeanimationterritoire/js/bootstrap.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/plateformeanimationterritoire/js/chat.js') }}"></script>
    <script type="text/javascript" src="http://www.activedev.net/js/jquery-1.4.2.min.js"></script>
    <script src="http://maps.google.com/maps/api/js?v=3.1&sensor=false" type="text/javascript"></script>
    <script src="{{ asset ('bundles/plateformeanimationterritoire/js/activemap.js') }}" type="text/javascript"></script>
    <script src="{{ asset ('bundles/plateformeanimationterritoire/js/infobox.js') }}" type="text/javascript"></script>
    <br/><br/>
    <iframe src="https://www.google.com/calendar/embed?height=400&amp;wkst=1&amp;bgcolor=%23ccccff&amp;src=9ejssm17nnub2m1qpc4air37j8%40group.calendar.google.com&amp;color=%23691426&amp;ctz=Europe%2FParis" style=" border:solid 1px #777 " width="500" height="400" frameborder="5" scrolling="no"></iframe>

    <h1 align="center">Carte des événements</h1>
    <div class="activemap"></div>

    <script type="text/javascript">
        $(function(){
            $('.activemap').ActiveMap({
                width:'600px',
                height:'450px',
                latInput:"map_lat",
                lngInput:"map_lng",
                zoomInput:"map_zoom",
                initPos: [48.471285, 1.014305], // [lat, lng]
                initZoom:10,
                navigationControl:true,
                scaleControl:true,
                pointerTitle:"Drag or click new position"
            });
        });
    </script>
{% endblock %}
</body>
</html>
